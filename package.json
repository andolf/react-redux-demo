{
  "name": "react-redux-demo",
  "private": true,
  "version": "0.0.1",
  "description": "A simple react-redux-demo.",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:prod": "webpack -p",
    "start": "node server & webpack-dev-server",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:e2e": "testcafe chrome test/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andolf/react-redux-demo.git"
  },
  "keywords": [
    "react",
    "redux",
    "demo"
  ],
  "author": "Christian Andolf <christian@weahead.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andolf/react-redux-demo/issues"
  },
  "homepage": "https://github.com/andolf/react-redux-demo#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "jest": "^23.6.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0",
    "testcafe": "^0.23.0",
    "testcafe-react-selectors": "^3.0.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.12.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.0.3",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6"
  }
