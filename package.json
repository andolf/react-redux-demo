{
  "name": "react-redux-demo",
  "private": true,
  "version": "0.0.1",
  "description": "A simple react-redux-demo.",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:prod": "webpack -p",
    "start": "node api & webpack-dev-server",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:e2e": "testcafe firefox:headless test/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andolf/react-redux-demo.git"
  },
  "keywords": [
    "react",
    "redux",
    "demo"
  ],
  "author": "Christian Andolf <christian@weahead.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andolf/react-redux-demo/issues"
  },
  "homepage": "https://github.com/andolf/react-redux-demo#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.0.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.9",
    "css-loader": "^3.3.2",
    "eslint": "^6.7.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.17.0",
    "express": "^4.17.1",
    "jest": "^25.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "style-loader": "^1.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "testcafe": "^1.7.0",
    "testcafe-react-selectors": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6"
  }
}
