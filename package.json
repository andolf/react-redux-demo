{
  "name": "react-redux-demo",
  "private": true,
  "version": "0.0.1",
  "description": "A simple react-redux-demo.",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:prod": "webpack -p",
    "start": "nodemon server & webpack-dev-server",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:e2e": "testcafe chrome test/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andolf/react-redux-demo.git"
  },
  "keywords": [
    "react",
    "redux",
    "demo"
  ],
  "author": "Christian Andolf <christian@weahead.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andolf/react-redux-demo/issues"
  },
  "homepage": "https://github.com/andolf/react-redux-demo#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.16.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^7.0.0",
    "express": "^4.14.1",
    "jest": "^20.0.0",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "style-loader": "^0.17.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "testcafe": "^0.15.0",
    "testcafe-react-selectors": "^0.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.12.0",
    "prop-types": "^15.5.9",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-modal": "^1.7.3",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^3.0.1"
  }
}
